{
  "name": "logardian",
  "version": "1.1.1",
  "description": "Minimalistic, fast, powerful node.js logger",
  "author": "Tema Rusanov",
  "private": false,
  "main": "./dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/temarusanov/logardian"
  },
  "scripts": {
    "prepare": "tsc",
    "format": "prettier --write \"lib/**/*.ts\"",
    "lint": "eslint \"{lib,test}/**/*.ts\" --fix",
    "lint:ts": "eslint \"{lib,test}/**/*.ts\" --fix --plugin tsc --rule \"tsc/config: [2, {configFile: \"./tsconfig.json\"}]\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "semantic-release": "semantic-release"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "lib",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "dotenv": "^10.0.0",
    "typescript": "^4.3.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^8.0.1",
    "@semantic-release/npm": "^8.0.0",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "apollo-server-env": "^3.1.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-tsc": "^2.0.0",
    "husky": "^4.3.8",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.1",
    "replace-in-file": "^6.2.0",
    "semantic-release": "^17.4.7",
    "standard-version": "^9.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0"
  }
}
